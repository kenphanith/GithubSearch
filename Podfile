# platform :ios, '9.0'

target 'GithubSearch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Rx
  pod "RxSwift"
  pod "RxCocoa"
  pod "ReactorKit"
  
  # Networking and JSON Parsing
  pod "APIKit"
  pod "Himotoki"
  pod "Argo"
  
  # Auto Layout constraints
  pod "SnapKit"
  
  # Database
  pod "Realm"
  pod "RealmSwift"
  
  # Swift Initializer
  pod "Then"

end
